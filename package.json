    "jest": "^29",
    "jest-junit": "^16",
    "suitecloud-unit-testing-stubs": "^1",
    "@oracle/suitecloud-unit-testing": "^1",
    "@oracle/suitecloud-cli": "^1",
    "@types/jest": "^29"
  }
}
